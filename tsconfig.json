{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "types": [
      "vite/client"
    ],
    "jsx": "preserve",
    "module": "commonjs",

    "baseUrl": ".",

    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "skipLibCheck": true
  },

}